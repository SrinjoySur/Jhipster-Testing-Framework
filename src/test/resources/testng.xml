<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Jhipster-Sample-App-Test-Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="com.epam.jhipstertap.listeners.TestListener"/>
    </listeners>
    <test name="Chrome Test">
        <parameter name="Browser" value="CHROME"/>
        <classes>
            <class name="com.epam.jhipstertap.runners.UiRunner"/>
        </classes>
    </test>
    <test name="EDGE Test">
        <parameter name="Browser" value="EDGE"/>
        <classes>
            <class name="com.epam.jhipstertap.runners.UiRunner"/>
        </classes>
    </test>
</suite>