<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Jhipster-Sample-App-Test-Suite">
    <listeners>
        <listener class-name="com.epam.ui.listeners.TestExecutionListener"/>
    </listeners>
    <test name="Smoke Test">
        <parameter name="browser" value="CHROME"/>
        <parameter name="username" value="admin"/>
        <parameter name="password" value="admin"/>
        <parameter name="rememberMe" value="true"/>
        <parameter name="name" value="anil"/>
        <parameter name="balance" value="12000.00"/>
        <parameter name="user" value="admin"/>
        <classes>
            <class name="com.epam.ui.tests.LoginTest"/>
            <class name="com.epam.ui.tests.BankAccountTests"/>
        </classes>
    </test>
</suite>